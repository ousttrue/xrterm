{
  "name": "xrterm",
  "version": "0.0.1",
  "description": "Terminal Emulator for xR environment",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall": "cd local_modules && (cd node-pty && npm install)",
    "server": "nodemon ./src/server/server.js",
    "client": "webpack-dev-server --host 0.0.0.0 --progress --colors --hot-only --inline --port 8080",
    "start": "concurrently --kill-others \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szk/xrterm.git"
  },
  "keywords": [
    "aframe",
    "vr",
    "xr",
    "terminal",
    "commandline",
    "xterm",
    "xrterm"
  ],
  "author": "szk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szk/xrterm/issues"
  },
  "homepage": "https://xrterm.com/",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "aframe-super-hot-html-loader": "^2.0.0",
    "aframe-super-hot-loader": "^1.6.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "concurrently": "^4.1.2",
    "diff-dom": "^4.1.3",
    "html-require-loader": "^1.0.1",
    "node-pty": "file:local_modules/node-pty",
    "nodemon": "^1.19.1",
    "super-nunjucks-loader": "^2.0.0",
    "webpack": "^4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "^3.8.0",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-sources": "1.0.1",
    "ws": "^6.2.1"
  },
  "devDependencies": {}
}
